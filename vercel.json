{
  "version": 2,
  "builds": [
    {
      "src": "marketing/landing.html",
      "use": "@vercel/static"
    },
    {
      "src": "server/guardianAPI.ts",
      "use": "@vercel/node"
    },
    {
      "src": "server/relayerServer.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/marketing/landing.html"
    },
    {
      "src": "/api/guardians/(.*)",
      "dest": "/server/guardianAPI.ts"
    },
    {
      "src": "/api/compliance/(.*)",
      "dest": "/server/guardianAPI.ts"
    },
    {
      "src": "/relay/(.*)",
      "dest": "/server/relayerServer.ts"
    },
    {
      "src": "/health",
      "dest": "/server/guardianAPI.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "RPC_MUMBAI": "@rpc_mumbai",
    "PINATA_API_KEY": "@pinata_api_key",
    "PINATA_SECRET_KEY": "@pinata_secret_key",
    "ZXT_TOKEN_ADDRESS_MUMBAI": "@zxt_token_address",
    "RELAYER_PRIVATE_KEY": "@relayer_private_key"
  },
  "functions": {
    "server/guardianAPI.ts": {
      "memory": 1024,
      "maxDuration": 30
    },
    "server/relayerServer.ts": {
      "memory": 512,
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    }
  ]
}