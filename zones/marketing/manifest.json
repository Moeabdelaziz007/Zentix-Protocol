      "name": "content_generator_tool",
      "description": "AI-powered content generation tool",
      "entryPoint": "tools/content_generator_tool.js"
    },
    {
      "name": "social_scheduler_tool",
      "description": "Social media scheduling tool",
      "entryPoint": "tools/social_scheduler_tool.js"
    }
  ],
  "governance": {
    "model": "hybrid",
    "votingThreshold": 70,
    "upgradePolicy": "voting"
  },
  "treasury": {
    "address": "0xabcdef1234567890abcdef1234567890abcdef12",
