{
  "name": "marketing",
  "version": "1.0.0",
  "type": "subzone",
  "description": "Marketing Zone - AI-powered marketing strategies and campaign management",
  "author": "Zentix Protocol",
  "license": "MIT",
  "entryPoint": "index.js",
  "dependencies": {
    "twitter-api-v2": "^1.15.0",
    "openai": "^4.0.0",
    "axios": "^1.4.0"
  },
  "services": [
    {
      "name": "social_media_service",
      "description": "Social media content generation and posting",
      "endpoint": "/api/social"
